{
  "name": "mascotai-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devlocal": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "android:sync": "npm run build && npx cap sync android",
    "android:run": "npm run build && npx cap sync android && npx cap run android",
    "android:open": "npm run build && npx cap sync android && npx cap open android",
    "build:clean": "rimraf dist && rimraf android && npm run build && npx cap add android && npx cap sync android",
    "assets:generate": "npx capacitor-assets generate --icon-background-color '#ffffff' --splash-background-color '#ffffff'"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/camera": "^6.0.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/geolocation": "^6.0.1",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4",
    "@sentry/capacitor": "^1.0.0",
    "@sentry/react": "8.55.0",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "firebase": "^12.8.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.469.0",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.5.1",
    "react-leaflet": "^5.0.0",
    "sweetalert2": "^11.15.10",
    "sweetalert2-react-content": "^5.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@eslint/js": "^9.17.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.0",
    "vite": "^7.3.1"
  },
  "overrides": {
    "xml2js": "^0.6.2",
    "tar": "^6.2.1",
    "semver": "^7.5.4",
    "json5": "^2.2.3"
  }
}
